            sh:order 20 ;
            sh:path ck:Substation.generationConstraint ],
        [ sh:datatype xsd:string ;
            sh:description "The name of the license area. This should be the EAN used in the license for the system operator." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path schema1:gtin13 ],
        [ sh:datatype unit:KiloA ;
            sh:description "The short circuit current (in kA) of a circuit breaker is the RMS value of current that a breaker can carry in a fully closed condition without damage, for the specified time interval under prescribed condition.  These ratings are based on thermal limitation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 26 ;
            sh:path ck:Substation.breakerShortCircuitCurrent ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The available reverse capacity (in MVA) for the substation from lower to higher voltage levels." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 29 ;
            sh:path ck:Substation.reversePowerFlowAvailableCapacity ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 39 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all currently quoted generation (in MVA) at the substation excluding any that are already contracted or connected. This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value for generationQuotedCapacity must be greater than or equal to the sum of the generationQuotedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 18 ;
