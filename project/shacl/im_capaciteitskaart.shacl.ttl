            sh:order 3 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:datatype xsd:string ;
            sh:description "Identifier for the postcode area. Contain four (4) numbers, followed by two (2) letters, lowercase. No spaces between the numbers and letters, i.e. \"1234ab\"." ;
