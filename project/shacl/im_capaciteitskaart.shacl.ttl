            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Storage.demandContractedCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:datatype unit:MegaW ;
            sh:description "The currently connected generation capacity (in MW)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Storage.generationConnectedCapacity ],
        [ sh:datatype unit:KiloV ;
            sh:description "The voltage the storage (in kV) is connected at." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path ck:Storage.voltage ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:datatype unit:MegaW ;
            sh:description "The currently connected demand capacity (in MW)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Storage.demandConnectedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "The currently active quoted generation capacity (in MW) above contracted (this is optional as connected/contracted generation may not have records of quotes and after a value is contracted this value is no longer relevant)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path ck:Storage.generationQuotedCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype xsd:boolean ;
            sh:description "True if the demand represents an aggregation of multiple demands all connected at a single voltage level." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path cim:Equipment.aggregate ],
        [ sh:datatype unit:MegaW ;
            sh:description "The currently contracted generation capacity (in MW) excluding any that are already contracted or connected (i.e. do not include any generation that is included in generationConnectedCapacity)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path ck:Storage.generationContractedCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "The technology of the connected generation e.g. PumpStorage, Battery." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path ck:Storage.technologyType ],
        [ sh:datatype unit:MegaW ;
            sh:description "The currently active quoted demand capacity (in MW) excluding contracted and connected (this is optional as connected/contracted generation may not have records of quotes and after a value is contracted this value is no longer relevant)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Storage.demandQuotedCapacity ] ;
    sh:targetClass ck:Storage .

cim:Circuit a sh:NodeShape ;
    sh:closed true ;
    sh:description "A circuit representing a connection between substations. This could be an HV circuit between stations or a circuit feeding multiple secondary stations from a primary. This data element is provided to support utilities that wish to share specfic or all circuits." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype unit:MegaW ;
            sh:description "Sum of all demand (in MW) currently quoted on the circuit excluding contracted and connected (i.e. does not include demandConnectedCapacity or demandContractedCapacity)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path ck:Circuit.demandQuotedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "The maximum load (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Circuit.demandMaximum ],
        [ sh:class ck:Constraint ;
            sh:description "An additional constraint on the Cicruit." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 18 ;
            sh:path ck:Circuit.OtherConstraints ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of requests for new grid connections and capacity increases in queue." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Circuit.requestsInQueue ],
        [ sh:datatype unit:MegaW ;
            sh:description "The 75th percentile of demand (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path ck:Circuit.demand75Percentile ],
        [ sh:datatype unit:MegaW ;
            sh:description "The 50th percentile of demand (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path ck:Circuit.demand50Percentile ],
        [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:datatype unit:MegaW ;
            sh:description "The minimum demand (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Circuit.demandMinimum ],
        [ sh:datatype xsd:string ;
            sh:description "mRID of the Substations the Circuit is to (optional if LV circuits are shown but no LV/Secondary substations). Where CIM data is being produced the identifiers attributes reflect the mRID of the Substations. Where no CIM mRID is yet being produced, the names of the Substations should be used." ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path ck:Circuit.toSubstation ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 22 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:class ck:Geometry ;
            sh:description "The geometry of the circuit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 19 ;
            sh:path ck:Circuit.Geometry ],
        [ sh:datatype unit:MegaW ;
            sh:description "The 25th percentile of demand (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path ck:Circuit.demand25Percentile ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype unit:KiloV ;
            sh:description "Voltage (in kV) of the circuit." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Circuit.voltage ],
        [ sh:datatype unit:MegaW ;
            sh:description "Summer rating (in MW) for minimum rated section in the circuit (i.e. limiting series element)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path ck:Circuit.summerRating ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all demand (in MW) currently connected on the circuit." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path ck:Circuit.demandConnectedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "The available demand (in MW) capacity on the circuit. This value is calculated based on the circuit rating, maximum demand, contracted capacity, and any other constraints on the network." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path ck:Circuit.demandAvailableCapacity ],
        [ sh:class cim:Work ;
            sh:description "All the works performed for this circuit." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 17 ;
            sh:path cim:Circuit.Works ],
        [ sh:description "A RAG definition of the demand constraints on the circuit. A utility should provide details on how this RAG status is calculated." ;
            sh:in ( ck:RAGKind.Red ck:RAGKind.Amber ck:RAGKind.Green ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 8 ;
            sh:path ck:Circuit.demandConstraint ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all demand (in MW) currently contracted on the circuit which are not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path ck:Circuit.demandContractedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "The 99th percentile of demand (in MW) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path ck:Circuit.demand99Percentile ],
        [ sh:datatype unit:MegaW ;
            sh:description "Winter rating (in MW) for minimum rated section in the circuit (i.e. limiting series element)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path ck:Circuit.winterRating ],
        [ sh:datatype xsd:string ;
            sh:description "mRID of the Substation the Circuit is from (used where CIM data is being produced and the mRID attributes are populated for Substations). Where no CIM mRID is yet being produced, the name of the Substation should be used." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path ck:Circuit.fromSubstation ] ;
    sh:targetClass cim:Circuit .

cim:NameType a sh:NodeShape ;
    sh:closed true ;
    sh:description "Type of name. Possible values for attribute 'name' are implementation dependent but standard profiles may specify types. An enterprise may have multiple IT systems each having its own local name for the same object, e.g.  a planning system may have different names from an EMS. An object may also have different names within the same IT system, e.g. localName as defined in CIM version 14. The definition from CIM14 is The localName is a human readable name of the object. It is a free text name local to a node in a naming hierarchy similar to a file directory structure. A power system related naming hierarchy may be Substation, VoltageLevel, Equipment etc. Children of the same parent in such a hierarchy have names that typically are unique among them." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class cim:NameTypeAuthority ;
            sh:description "Authority responsible for managing names of this type." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path cim:NameType.NameTypeAuthority ],
        [ sh:datatype xsd:string ;
            sh:description "Description of the name type." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cim:NameType.description ] ;
    sh:targetClass cim:NameType .

cim:NameTypeAuthority a sh:NodeShape ;
    sh:closed true ;
    sh:description "Authority responsible for creation and management of names of a given type; typically an organization or an enterprise system." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Description of the name type authority." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cim:NameTypeAuthority.description ] ;
    sh:targetClass cim:NameTypeAuthority .

ck:Constraint a sh:NodeShape ;
    sh:closed true ;
    sh:description "An additional constraint/restriction on the Substation or Circuit. This is intended for non-numeric constraints e.g. to denote physical restrictions on reinforcement." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Free text for any additional descriptive data about the constraint." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Constraint.description ],
        [ sh:datatype xsd:string ;
            sh:description "The type of constraint (enumerated field to allow for easier processing/identification). Limited to Voltage, Physical or Other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Constraint.type ] ;
    sh:targetClass ck:Constraint .

ck:Coordinate a sh:NodeShape ;
    sh:closed true ;
    sh:description "A coordinate of latitude/longitude." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:float ;
            sh:description "The longitude value of the coordinate as a single latitude/longitude point. For a GeoJSON Heatmap format this data will be part of the LineString geometry." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Coordinate.longitude ],
        [ sh:datatype xsd:float ;
            sh:description "The latitude value of the coordinate as a single latitude/longitude point. For a GeoJSON Heatmap format this data will be part of the LineString geometry." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Coordinate.latitude ] ;
    sh:targetClass ck:Coordinate .

ck:Geometry a sh:NodeShape ;
    sh:closed true ;
    sh:description "The geometry of the line, this will be one or more Lines i.e. series of ordered coordinates. Multiple Lines allowed to allow for radial geometries. It is likely that this will be rendered as a GeoJSON geometry in that format." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The geometry type (LineString, MultiLineString or Polygon)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Geometry.type ],
        [ sh:class ck:Line ;
            sh:description "The Line elements." ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path ck:Geometry.Lines ] ;
    sh:targetClass ck:Geometry .

cim:DateTimeInterval a sh:NodeShape ;
    sh:closed true ;
    sh:description "Interval between two date and time points, where the interval includes the start time but excludes end time." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:date ;
            sh:description "Start date and time of this interval.  The start date and time is included in the defined interval." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path cim:DateTimeInterval.start ],
        [ sh:datatype xsd:date ;
            sh:description "End date and time of this interval. The end date and time where the interval is defined up to, but excluded." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cim:DateTimeInterval.end ] ;
    sh:targetClass cim:DateTimeInterval .

cim:Substation a sh:NodeShape ;
    sh:closed true ;
    sh:description "A Substation with connect demand, generation or storage." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype unit:MegaW ;
            sh:description "The 50th percentile of demanding (in MW) at the substation. This is a normalised value calculated using historic measured values during normal operating conditions. The data represents net demand corrected for any generation at the substation, demand diversity factors, and any other internal processing normally undertaken. All substations at 50kV and above must include demand data; for substations below 50kV, if corrected, net demand data is not available, the value should be omitted." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path ck:Substation.demand50Percentile ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all generation (in MW) currently contracted at the substation which is not yet connected (i.e. do not include any generation that is included in generationConnectedCapacity). This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value for generationContractedCapacity must be greater than or equal to the sum of the generationContractedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path ck:Substation.generationContractedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all generation (in MW) currently connected at the substation. This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value forgenerationConnectedCapacity must be greater than or equal to the sum of the generationConnectedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path ck:Substation.generationConnectedCapacity ],
        [ sh:class ck:Generator ;
            sh:description "The generators at the station covering connected, contracted and quoted. This data is optional and may aggregated into the substation attributes, but utilities may optionally provide details on individual generators." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 31 ;
            sh:path ck:Substation.Generators ],
        [ sh:datatype unit:MegaW ;
            sh:description "Available generation capacity (in MW) at the substation. This value is calculated based on total firm generation, capacity, demand, connected capacity, contracted capacity, and any other constraints on the network." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path ck:Substation.generationAvailableCapacity ],
        [ sh:description "A RAG definition of the demand constraints at the substation. A utility should provide details on how this RAG status is calculated as part of the accompanying documentation." ;
            sh:in ( ck:RAGKind.Red ck:RAGKind.Amber ck:RAGKind.Green ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 18 ;
            sh:path ck:Substation.demandConstraint ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all currently quoted generation (in MW) at the substation excluding any that are already contracted or connected. This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value for generationQuotedCapacity must be greater than or equal to the sum of the generationQuotedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path ck:Substation.generationQuotedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "Sum of all demand (in MW) currently contracted at the substation which is not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity). This includes individual demands and storage that are explicitly defined and aggregate value of other connect demands and storage. The value for demandContractedCapacity must be greater than or equal to the sum of the demandContractedCapacity values in the Demand and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path ck:Substation.demandContractedCapacity ],
        [ sh:datatype unit:MegaW ;
            sh:description "The maximum demand (in MW) at the substation. This is a normalised value calculated using historic measured values during normal operating conditions. The data represents net demand without removing the impact of any generation on observed demand at the substation, demand diversity factors, and any other internal processing normally undertaken. All substations at 33kV and above must include demand data; for substations below 33kV, if corrected, net demand data is not available, the value should be omitted. Users wishing to see separate summer/winter peaks can obtain this from the data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path ck:Substation.demandMaximum ],
        [ sh:datatype unit:MegaW ;
            sh:description "The available demand capacity (in MW) at the substation. This value is calculated based on total firm capacity, maximum demand, connected capacity, quoted capacity, contracted capacity, and any other constraints on the network. The value should reflect the capacity a network operator would make available to a connecting party, which may be calculated for the most onerous season, or a value based on more detailed analysis and internal policies." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
