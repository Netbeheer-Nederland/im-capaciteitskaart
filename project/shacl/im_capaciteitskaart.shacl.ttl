            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently active quoted demand capacity (in MVA) excluding contracted and connected (this is optional as connected/contracted generation may not have records of quotes and after a value is contracted this value is no longer relevant)." ;
