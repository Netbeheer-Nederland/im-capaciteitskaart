ck:Curtailment a sh:NodeShape ;
    sh:closed true ;
    sh:description "Curtailment of demand or generation capacity at the substation with optional day/time and season parameters." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The type of curtailment (enumerated field to allow for easier processing/identification)." ;
            sh:in ( ck:CurtailmentKind.Demand ck:CurtailmentKind.Generation ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ck:Curtailment.type ],
        [ sh:datatype unit:MegaW ;
            sh:description "The amount the demand/generation capacity (in MW) is curtailed by." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Curtailment.curtailedBy ],
        [ sh:datatype xsd:time ;
            sh:description "The start time of a curtailment (inclusive)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:description "The season this curtailment applies during." ;
            sh:in ( ck:SeasonKind.Spring ck:SeasonKind.Summer ck:SeasonKind.Autumn ck:SeasonKind.Winter ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:datatype xsd:time ;
            sh:description "The end time of a curtailment (exclusive)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:datatype xsd:string ;
            sh:description "The technology the curtailment applies to e.g. for generation CHP, Wind, PV, Battery." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Curtailment.technologyType ],
        [ sh:description "The type of day the curtailment applies during (weekday or weekend)." ;
            sh:in ( ck:DayKind.Weekday ck:DayKind.Weekend ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:datatype xsd:string ;
            sh:description "Free text for any additional descriptive data about the curtailment." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Curtailment.description ] ;
    sh:targetClass ck:Curtailment .

