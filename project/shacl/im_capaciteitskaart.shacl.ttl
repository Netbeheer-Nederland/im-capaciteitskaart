ck:Curtailment a sh:NodeShape ;
    sh:closed true ;
    sh:description "Curtailment of demand or generation capacity at the substation with optional day/time and season parameters." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype unit:MegaV-A ;
            sh:description "The amount the demand/generation capacity (in MVA) is curtailed by." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Curtailment.curtailedBy ],
        [ sh:datatype xsd:string ;
            sh:description "Free text for any additional descriptive data about the curtailment." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Curtailment.description ],
        [ sh:datatype xsd:time ;
            sh:description "The end time of a curtailment (exclusive)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:datatype xsd:time ;
            sh:description "The start time of a curtailment (inclusive)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:description "The season this curtailment applies during." ;
            sh:in ( ck:SeasonKind.Spring ck:SeasonKind.Summer ck:SeasonKind.Autumn ck:SeasonKind.Winter ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ],
        [ sh:datatype xsd:string ;
            sh:description "The technology the curtailment applies to e.g. for generation CHP, Wind, PV, Battery." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Curtailment.technologyType ],
        [ sh:description "The type of curtailment (enumerated field to allow for easier processing/identification)." ;
            sh:in ( ck:CurtailmentKind.Demand ck:CurtailmentKind.Generation ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path ck:Curtailment.type ],
        [ sh:description "The type of day the curtailment applies during (weekday or weekend)." ;
            sh:in ( ck:DayKind.Weekday ck:DayKind.Weekend ) ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path <http://data.netbeheernederland.nl/capaciteitskaart/def#Curtailment.> ] ;
    sh:targetClass ck:Curtailment .

ck:Demand a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents an individual or aggregated demand at the substation. This may represent the aggregation of all generation at a voltage level or individual connected/contracted/quoted demand." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype unit:MegaV-A ;
            sh:description "The currently contracted demand capacity (in MVA) which is not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Demand.demandContractedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently active quoted demand capacity (in MVA) excluding contracted and connected (this is optional as connected/contracted generation may not have records of quotes and after a value is contracted this value is no longer relevant)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Demand.demandQuotedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently connected demand capacity (in MVA)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Demand.demandConnectedCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype unit:KiloV ;
            sh:description "The voltage the demand (in kV) is connected at." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Demand.voltage ],
        [ sh:datatype xsd:boolean ;
            sh:description "True if the demand represents an aggregation of multiple demands all connected at a single voltage level." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path cim:Equipment.aggregate ],
        [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path cim:IdentifiedObject.Names ] ;
    sh:targetClass ck:Demand .

