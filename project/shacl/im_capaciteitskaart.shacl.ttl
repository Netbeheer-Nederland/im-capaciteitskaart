            sh:order 8 ;
            sh:path cim:Equipment.aggregate ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently connected generation capacity (in MVA)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Storage.generationConnectedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently active quoted generation capacity (in MVA) above contracted (this is optional as connected/contracted generation may not have records of quotes and after a value is contracted this value is no longer relevant)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path ck:Storage.generationQuotedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The currently contracted demand capacity (in MVA) which is not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Storage.demandContractedCapacity ] ;
    sh:targetClass ck:Storage .

cim:Circuit a sh:NodeShape ;
    sh:closed true ;
    sh:description "A circuit representing a connection between substations. This could be an HV circuit between stations or a circuit feeding multiple secondary stations from a primary. This data element is provided to support utilities that wish to share specfic or all circuits." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:class ck:Constraint ;
            sh:description "An additional constraint on the Cicruit." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 13 ;
            sh:path ck:Circuit.OtherConstraints ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently connected on the circuit." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path ck:Circuit.demandConnectedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Winter rating (in MVA) for minimum rated section in the circuit (i.e. limiting series element)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path ck:Circuit.winterRating ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Summer rating (in MVA) for minimum rated section in the circuit (i.e. limiting series element)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path ck:Circuit.summerRating ],
        [ sh:datatype unit:KiloV ;
            sh:description "Voltage (in kV) of the circuit." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:Circuit.voltage ],
        [ sh:class ck:Geometry ;
            sh:description "The geometry of the circuit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path ck:Circuit.Geometry ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The available demand (in MVA) capacity on the circuit. This value is calculated based on the circuit rating, maximum demand, contracted capacity, and any other constraints on the network." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path ck:Circuit.demandAvailableCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The minimum demand (in MVA) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path ck:Circuit.demandMinimum ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 17 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:description "A RAG definition of the demand constraints on the circuit. A utility should provide details on how this RAG status is calculated." ;
            sh:in ( ck:RAGKind.Red ck:RAGKind.Amber ck:RAGKind.Green ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path ck:Circuit.demandConstraint ],
        [ sh:datatype xsd:string ;
            sh:description "mRID of the Substations the Circuit is to (optional if LV circuits are shown but no LV/Secondary substations). Where CIM data is being produced the identifiers attributes reflect the mRID of the Substations. Where no CIM mRID is yet being produced, the names of the Substations should be used." ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path ck:Circuit.toSubstation ],
        [ sh:datatype xsd:string ;
            sh:description "mRID of the Substation the Circuit is from (used where CIM data is being produced and the mRID attributes are populated for Substations). Where no CIM mRID is yet being produced, the name of the Substation should be used." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path ck:Circuit.fromSubstation ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently quoted on the circuit excluding contracted and connected (i.e. does not include demandConnectedCapacity or demandContractedCapacity)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path ck:Circuit.demandQuotedCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The maximum load (in MVA) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path ck:Circuit.demandMaximum ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently contracted on the circuit which are not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path ck:Circuit.demandContractedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The 25th percentile of demand (in MVA) on the circuit. This is a value calculated using historic measured values and would not necessarily be corrected for generation on the circuit and normal operating conditions." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path ck:Circuit.demand25Percentile ] ;
    sh:targetClass cim:Circuit .

cim:NameType a sh:NodeShape ;
    sh:closed true ;
    sh:description "Type of name. Possible values for attribute 'name' are implementation dependent but standard profiles may specify types. An enterprise may have multiple IT systems each having its own local name for the same object, e.g.  a planning system may have different names from an EMS. An object may also have different names within the same IT system, e.g. localName as defined in CIM version 14. The definition from CIM14 is The localName is a human readable name of the object. It is a free text name local to a node in a naming hierarchy similar to a file directory structure. A power system related naming hierarchy may be Substation, VoltageLevel, Equipment etc. Children of the same parent in such a hierarchy have names that typically are unique among them." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Description of the name type." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cim:NameType.description ],
        [ sh:class cim:NameTypeAuthority ;
            sh:description "Authority responsible for managing names of this type." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path cim:NameType.NameTypeAuthority ] ;
    sh:targetClass cim:NameType .

cim:NameTypeAuthority a sh:NodeShape ;
    sh:closed true ;
    sh:description "Authority responsible for creation and management of names of a given type; typically an organization or an enterprise system." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Description of the name type authority." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cim:NameTypeAuthority.description ] ;
    sh:targetClass cim:NameTypeAuthority .

cim:PowerTransformer a sh:NodeShape ;
    sh:closed true ;
    sh:description "A transformer at a substation. Optional but can be useful if ratings and capacity are required on a per-transformer level." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:boolean ;
            sh:description "True if the demand represents an aggregation of multiple demands all connected at a single voltage level." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path cim:Equipment.aggregate ],
        [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:class cim:Name ;
            sh:description "All names of this identified object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path cim:IdentifiedObject.Names ],
        [ sh:datatype xsd:integer ;
            sh:description "The primary, secondary (and optional tertiary) voltages of the transformer." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path ck:PowerTransformer.voltages ],
        [ sh:class ck:Rating ;
            sh:description "The ratings of the transformer as defined by the Ratings element." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path ck:PowerTransformer.Ratings ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path cim:IdentifiedObject.mRID ] ;
    sh:targetClass cim:PowerTransformer .

cim:Substation a sh:NodeShape ;
    sh:closed true ;
    sh:description "A Substation with connect demand, generation or storage." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently connected at the substation. This includes individual demands and storage that are explicitly defined and aggregate value of other connect demands and storage. The value for demandConnectedCapacity must be greater than or equal to the sum of the demandConnectedCapacity values in the demand and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path ck:Substation.demandConnectedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently quoted at the substation excluding contracted and connected (i.e. does not include demandConnectedCapacity or demandContractedCapacity). This includes individual demands and storage that are explicitly defined and aggregate value of other connect demands and storage. The value for demandQuotedCapacity must be greater than or equal to the sum of the demandQuotedCapacity values in the demand and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path ck:Substation.demandQuotedCapacity ],
        [ sh:class ck:Coordinate ;
            sh:description "The longitude/latitude value of the substation as a single latitude/longitude point. For a GeoJSON Heatmap format this data will be part of the feature itself allowing for more complex geometry." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path ck:Substation.Coordinate ],
        [ sh:datatype xsd:string ;
            sh:description "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 37 ;
            sh:path cim:IdentifiedObject.description ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all demand (in MVA) currently contracted at the substation which is not yet connected (i.e. do not include any demand that is included in demandConnectedCapacity). This includes individual demands and storage that are explicitly defined and aggregate value of other connect demands and storage. The value for demandContractedCapacity must be greater than or equal to the sum of the demandContractedCapacity values in the Demand and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path ck:Substation.demandContractedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The 25th percentile of demanding (in MVA) at the substation. This is a normalised value calculated using historic measured values during normal operating conditions. The data represents net demand corrected for any generation at the substation, demand diversity factors, and any other internal processing normally undertaken. All substations at 50kV and above must include demand data; for substations below 50kV, if corrected, net demand data is not available, the value should be omitted." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path ck:Substation.demand25Percentile ],
        [ sh:datatype unit:KiloA ;
            sh:description "The short-circuit current (in kA) that the breakers are capable of breaking.  It is expressed in KA RMS at contact separation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path ck:Substation.breakerBreakingCapacity ],
        [ sh:description "The type of the substation." ;
            sh:in ( ck:SubstationKind.Onderstation ck:SubstationKind.Middenspanningsruimte ck:SubstationKind.Laagspanningskast ck:SubstationKind.Regelstation ck:SubstationKind.Schakelstation ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path ck:Substation.type ],
        [ sh:datatype unit:KiloA ;
            sh:description "The rated normal current (in kA) of the circuit breaker is the RMS value of the current with which the circuit breaker shall be able to carry continuously." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path ck:Substation.breakerRatedCurrent ],
        [ sh:description "The limiting factor for generation constraints, either thermal or fault level for the generationConstraint value." ;
            sh:in ( ck:LimitingFactorKind.Thermal ck:LimitingFactorKind.Voltage ck:LimitingFactorKind.FaultLevel ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 21 ;
            sh:path ck:Substation.generationConstraintLimitingFactor ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The available demand capacity (in MVA) at the substation. This value is calculated based on total firm capacity, maximum demand, connected capacity, quoted capacity, contracted capacity, and any other constraints on the network. The value should reflect the capacity a network operator would make available to a connecting party, which may be calculated for the most onerous season, or a value based on more detailed analysis and internal policies." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path ck:Substation.demandAvailableCapacity ],
        [ sh:class cim:PowerTransformer ;
            sh:description "The transformers connected at the substation." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 35 ;
            sh:path ck:Substation.Transformers ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The firm capacity (in MVA) for demand at the substation. Any constraints that impact this capacity should be listed as individual constraints linked to the substation." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path ck:Substation.demandFirmCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all generation (in MVA) currently contracted at the substation which is not yet connected (i.e. do not include any generation that is included in generationConnectedCapacity). This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value for generationContractedCapacity must be greater than or equal to the sum of the generationContractedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path ck:Substation.generationContractedCapacity ],
        [ sh:class ck:Generator ;
            sh:description "The generators at the station covering connected, contracted and quoted. This data is optional and may aggregated into the substation attributes, but utilities may optionally provide details on individual generators." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 32 ;
            sh:path ck:Substation.Generators ],
        [ sh:description "A RAG definition of the demand constraints at the substation. A utility should provide details on how this RAG status is calculated as part of the accompanying documentation." ;
            sh:in ( ck:RAGKind.Red ck:RAGKind.Amber ck:RAGKind.Green ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 13 ;
            sh:path ck:Substation.demandConstraint ],
        [ sh:class ck:Constraint ;
            sh:description "An additional constraint on the Substation." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 34 ;
            sh:path ck:Substation.OtherConstraints ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "Sum of all generation (in MVA) currently connected at the substation. This includes individual generation and storage that are explicitly defined and aggregate value of other connect generation and storage. The value forgenerationConnectedCapacity must be greater than or equal to the sum of the generationConnectedCapacity values in the Generator and Storage elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path ck:Substation.generationConnectedCapacity ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The total firm generation capacity (in MVA) of the substation.  Any constraints that impact this capacity should be listed as individual constraints linked to the substation. The approach used to calculate this value should be defined in accompanying documentation." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path ck:Substation.generationFirmCapacity ],
        [ sh:datatype xsd:string ;
            sh:description "Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended. For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 38 ;
            sh:path cim:IdentifiedObject.mRID ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The minimum demand (in MVA) at the substation. This is a normalised value calculated using historic measured values during normal operating conditions. The data represents net demand corrected for any generation at the substation, demand diversity factors, and any other internal processing normally undertaken. All substations at 33kV and above must include demand data; for substations below 33kV, if corrected, net demand data is not available, the value should be omitted. Users wishing to see separate summer/winter peaks can obtain this from the data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path ck:Substation.demandMinimum ],
        [ sh:class ck:Curtailment ;
            sh:description "Curtailments at the Substation." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 36 ;
            sh:path ck:Substation.Curtailments ],
        [ sh:datatype unit:MegaV-A ;
            sh:description "The maximum demand (in MVA) at the substation. This is a normalised value calculated using historic measured values during normal operating conditions. The data represents net demand without removing the impact of any generation on observed demand at the substation, demand diversity factors, and any other internal processing normally undertaken. All substations at 33kV and above must include demand data; for substations below 33kV, if corrected, net demand data is not available, the value should be omitted. Users wishing to see separate summer/winter peaks can obtain this from the data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path ck:Substation.demandMaximum ],
        [ sh:datatype xsd:integer ;
            sh:description "Specified time interval (in seconds) the Short Circuit Current can be carried for without damage." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 27 ;
            sh:path ck:Substation.breakerShortCircuitDuration ],
        [ sh:description "The limiting factor for demand constraints, either thermal, voltage or fault level for the demandConstraint value." ;
            sh:in ( ck:LimitingFactorKind.Thermal ck:LimitingFactorKind.Voltage ck:LimitingFactorKind.FaultLevel ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 14 ;
            sh:path ck:Substation.demandConstraintLimitingFactor ],
        [ sh:datatype unit:KiloA ;
            sh:description "The available capacity (in kA) of the circuit breaker." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 25 ;
            sh:path ck:Substation.breakerAvailableCapacity ],
        [ sh:datatype unit:KiloA ;
            sh:description "The making current (in kA) of the circuit breaker, when closed on a short circuit, is the peak value of the maximum current wave (including dc component) in the first cycle of the current after the circuit is closed by the circuit breaker." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path ck:Substation.breakerMakingCapacity ],
        [ sh:datatype xsd:string ;
